<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xml.amadeus.com/PNRACC_14_2_1A" xmlns="http://xml.amadeus.com/PNRACC_14_2_1A" elementFormDefault="qualified">
  <xs:element name="PNR_Reply">
    <xs:annotation>
      <xs:documentation xml:lang="en">Display the content of SBR.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="generalErrorInfo" minOccurs="0" maxOccurs="99" type="ErrorGroupType_212789G">
          <xs:annotation>
            <xs:documentation xml:lang="en">specify general error</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="pnrHeader" minOccurs="0" maxOccurs="198">
          <xs:annotation>
            <xs:documentation xml:lang="en">PNR header</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="reservationInfo" type="ReservationControlInformationType_131820S">
                <xs:annotation>
                  <xs:documentation xml:lang="en">specify the reference to a reservation</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="referenceForRecordLocator" minOccurs="0" type="ReferenceInfoType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">provide specific reference identification</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="securityInformation" minOccurs="0" type="ReservationSecurityInformationType">
          <xs:annotation>
            <xs:documentation xml:lang="en">specify the amadeus PNR record locator security information for different pnr elements .</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="queueInformations" minOccurs="0" type="QueueType">
          <xs:annotation>
            <xs:documentation xml:lang="en">specify a queue , that is  an office , a queue number , a category number ,and a date range number or a date .This segment can be used in an output message.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="numberOfUnits" minOccurs="0" type="NumberOfUnitsTypeI">
          <xs:annotation>
            <xs:documentation xml:lang="en">specify the number of units required</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="pnrType" minOccurs="0" type="CodedAttributeType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Conveys PNR special types</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="freetextData" minOccurs="0" maxOccurs="3" type="LongFreeTextType">
          <xs:annotation>
            <xs:documentation xml:lang="en">provide free form or coded long text information</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="pnrHeaderTag" minOccurs="0" type="StatusType_186270S">
          <xs:annotation>
            <xs:documentation xml:lang="en">Conveys the Header tags of the PNR.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="freeFormText" minOccurs="0" maxOccurs="99" type="InteractiveFreeTextTypeI_136698S">
          <xs:annotation>
            <xs:documentation xml:lang="en">provide free from or coded text information</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="historyData" minOccurs="0" maxOccurs="999" type="PnrHistoryDataType">
          <xs:annotation>
            <xs:documentation xml:lang="en">specify an amadeus PNR history data</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="sbrPOSDetails" type="POSGroupType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Point of Sale Information at SBR level. Owner of the PNR.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="sbrCreationPosDetails" type="POSGroupType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Point of Sale Information at SBR level. Creator of the PNR.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="sbrUpdatorPosDetails" type="POSGroupType">
          <xs:annotation>
            <xs:documentation xml:lang="en">Point of Sale Information at SBR level. Updator of the PNR.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="technicalData" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="en">Contains technical data associated to this PNR: --) Purge Date of the PNR --) enveloppe number at last EOT</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="enveloppeNumberData" type="SequenceDetailsTypeU">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Contains the enveloppe number of the PNR, issue at last EOT.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="lastTransmittedEnvelopeNumber" minOccurs="0" type="PnrHistoryDataType_27157S">
                <xs:annotation>
                  <xs:documentation xml:lang="en">CS assumption on last transmitted envelope number</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="purgeDateData" minOccurs="0" type="StructuredDateTimeInformationType_27086S">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Contains the Purge Date of the PNR at the time of the retrieval</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="generalPNRInformation" minOccurs="0" type="StatusTypeI_32775S">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Contains general information relative to the state of the PNR</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="travellerInfo" minOccurs="0" maxOccurs="100">
          <xs:annotation>
            <xs:documentation xml:lang="en">specify ll name elements</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="elementManagementPassenger" type="ElementManagementSegmentType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">specify the PNR segments/elements references and action to apply</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="passengerData" minOccurs="0" maxOccurs="2">
                <xs:annotation>
                  <xs:documentation xml:lang="en">This group contains passenger related information:  - name  - date of birth First repetition is usually for adult and second for related infant.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="travellerInformation" type="TravellerInformationType_185946S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">specify traveler and personal details relating to a traveler</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="groupCounters" minOccurs="0" type="NumberOfUnitsType_76106S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">In case of group, contains the group counters (Booked, Canceled, Split)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="dateOfBirth" minOccurs="0" type="DateAndTimeInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Inf/Child date of birth (DDMMYYYY). For instance 01122007</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="enhancedPassengerData" minOccurs="0" maxOccurs="2">
                <xs:annotation>
                  <xs:documentation xml:lang="en">This group contains passenger related information:  - name  - date of birth First repetition is usually for adult and second for related infant.  This group is used to carry: - rich names - and/or multiple names (Native name(s)), Universal name, for a same passenger. - or name (rich or roman) with title separated from the firstname.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="enhancedTravellerInformation" type="EnhancedTravellerInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Specify traveler and personal details relating to a traveler.  Will be filled imperatively if name is UTF-8 encoded.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="groupCountersInEnhancedPaxData" minOccurs="0" type="NumberOfUnitsType_76106S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">In case of group, contains the group counters (Booked, Canceled, Split)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="dateOfBirthInEnhancedPaxData" minOccurs="0" type="DateAndTimeInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Inf/Child date of birth (DDMMYYYY). For instance 01122007</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="nameError" minOccurs="0" type="ErrorGroupType_223552G">
                <xs:annotation>
                  <xs:documentation xml:lang="en">Specify error at name level</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="originDestinationDetails" minOccurs="0" maxOccurs="50">
          <xs:annotation>
            <xs:documentation xml:lang="en">specify connected/non connected air segments</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="originDestination" type="OriginAndDestinationDetailsTypeI_3061S">
                <xs:annotation>
                  <xs:documentation xml:lang="en">convey origin and destination of a journey</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="itineraryInfo" minOccurs="0" maxOccurs="99">
                <xs:annotation>
                  <xs:documentation xml:lang="en">specify all segments</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="elementManagementItinerary" type="ElementManagementSegmentType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">specify the PNR segments/elements references and action to apply</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="travelProduct" minOccurs="0" type="TravelProductInformationTypeI_193100S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">specify details related to a product</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="itineraryMessageAction" minOccurs="0" type="MessageActionDetailsType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">specify the message type and business function</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="itineraryReservationInfo" minOccurs="0" type="ReservationControlInformationTypeI_196503S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">specify a reference to a reservation</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="relatedProduct" minOccurs="0" type="RelatedProductInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">indicate quantity and action required in relation to a product</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="elementsIndicators" minOccurs="0" type="StatusType_150135S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Indicator at element level: - CGB (chargeable indicator) - TSA (Secure Flight indicator)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="reasonForIssuanceCode" minOccurs="0" type="PricingOrTicketingSubsequentType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To Convey the Reason for Issuance Code (RFIC) and Reason For Issuance Sub code (RFISC)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="flightDetail" minOccurs="0" type="AdditionalProductDetailsTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">convey additional information concerning an airline flight</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="cabinDetails" minOccurs="0" type="CabinDetailsType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Contain the cabin class details</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="selectionDetails" minOccurs="0" type="SelectionDetailsTypeI_2067S">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">specify details for making a selection</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="itineraryfreeFormText" minOccurs="0" maxOccurs="7" type="InteractiveFreeTextType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">provide free from or coded text information</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="itineraryFreetext" minOccurs="0" type="LongFreeTextType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">provide free form or coded long text information</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="hotelProduct" minOccurs="0" type="HotelProductInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">specify the details for hotel transaction</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="rateInformations" minOccurs="0" type="RateInformationType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">specify the Rate information</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="generalOption" minOccurs="0" maxOccurs="199" type="GeneralOptionType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">specify one option</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="country" minOccurs="0" type="CountryCodeListType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">provide the ISO code of a country</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="taxInformation" minOccurs="0" maxOccurs="3" type="TaxTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">To specify details relating to tax(es).</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="customerTransactionData" minOccurs="0" type="CustomerTransactionDataType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">specify operating flight additional information</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="yieldGroup" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Details of the yield data.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="yieldData" type="ODKeyPerformanceDataType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">It contains some specific air segment's indicators data, not present in yieldDataGroup</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="yieldDataGroup" minOccurs="0" type="ONDType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Details of the yield data.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="legInfo" minOccurs="0" maxOccurs="99">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="markerLegInfo" type="FlightSegmentDetailsTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Marks separation to avoid Ambiguity between TVL segments.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="legTravelProduct" type="TravelProductInformationTypeI_99362S" />
                          <xs:element name="interactiveFreeText" minOccurs="0" maxOccurs="2" type="InteractiveFreeTextTypeI_99363S" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="flixInfo" minOccurs="0" maxOccurs="4" type="FLIXType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Group used to carry FLIX information associated to the segment</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="dateTimeDetails" minOccurs="0" type="DateAndTimeInformationTypeI">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Provide Date and Time Details relative to the Itinerary</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="lccTypicalData" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Contains data pertaining to LCC segments</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="lccFareData" type="TariffInformationTypeI_28460S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Fare data obtained from D/A availabilty (mapped under PRXP20LCC structure)</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="lccConnectionData" minOccurs="0" type="ItemReferencesAndVersionsType_6550S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Connection key tattoo if any</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="insuranceInformation" minOccurs="0" maxOccurs="198">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">specify insurance information</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="insuranceName" type="InsuranceNameType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">contain data related to each passenger</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="insuranceMonetaryInformation" minOccurs="0" type="MonetaryInformationTypeI_1689S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">To specify monetary information</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="insurancePremiumInfo" minOccurs="0" type="TravellerInsuranceInformationType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Specify an Amadeus PNR Ticket element</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="insuranceDocInfo" minOccurs="0" type="TravellerDocumentInformationTypeU">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">provide traveller document information</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="insuranceDetails" minOccurs="0" type="InsuranceBusinessDataType">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">This group handles all Insurance structured details.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="hotelReservationInfo" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">This group contains all the new segments to be integrated in PNRACC in case of a PNR retrieval</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="hotelPropertyInfo" type="HotelPropertyType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This segment is used to convey the hotel property information.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="companyIdentification" type="CompanyInformationType">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This segment is used to convey the hotel chain code and name.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="requestedDates" type="StructuredPeriodInformationType_185758S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This segment is used to convey the dates.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="roomRateDetails" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This group is used to convey the room /rate details.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="roomInformation" type="HotelRoomType">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">This segment is used to convey the room information.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="children" minOccurs="0" maxOccurs="99" type="ChildrenGroupType">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">This group is used to conveys list of children</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="tariffDetails" type="TariffInformationTypeI">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">This segment is used to convey the tariff/rate details.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="rateCodeIndicator" minOccurs="0" type="RuleInformationTypeU">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">This segment is used to convey the rate code indicator.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="cancelOrConfirmNbr" maxOccurs="3" type="ReservationControlInformationTypeI_196503S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This segment is used to convey the confirmation number or the cancellation number. control type (9958) is: - 2 for Confirmation reference - X for cancellation reference - O for on request reference</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="roomstayIndex" minOccurs="0" type="ItemNumberTypeU_33258S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">indicate the roomstay index in case of groups</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="bookingSource" type="UserIdentificationType_21014S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This segment is used to convey the booking source.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="billableInfo" minOccurs="0" type="BillableInformationTypeU">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This segment is used to convey the billable information</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="customerInfo" minOccurs="0" type="ConsumerReferenceInformationTypeI">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This segment is used to convey the customer reference number</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="frequentTravellerInfo" minOccurs="0" type="FrequentTravellerIdentificationCodeType_38226S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This segment is used to convey the frequent traveler number.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="guaranteeOrDeposit" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This group is used for the guarantee or the deposit</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="paymentInfo" type="PaymentInformationTypeI">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">This segment is used to convey the guarantee or deposit information</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="creditCardInfo" minOccurs="0" type="FormOfPaymentTypeI_29553S">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">This segment is used to convey the credit card information.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="textOptions" minOccurs="0" maxOccurs="5" type="MiscellaneousRemarksType_36733S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This segment is used to convey additional information which are entered by the agent and stored on the hotel booking.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="savingAmountInfo" minOccurs="0" type="MonetaryInformationTypeI_1689S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This segment is used to convey the saving amount/percentage information.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="writtenConfirmationContact" minOccurs="0" type="ContactInformationTypeU">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This segment is used to convey the fax or the E-Mail to receive the written confirmation</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="writtenConfirmationInfo" minOccurs="0" type="NameAndAddressBatchTypeU">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">This segment is used to convey the name and address of the party to recieve the written confirmation</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="documentInformationDetails" minOccurs="0" type="DocumentInformationDetailsTypeI_9936S">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">documentInformationDetails</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="arrivalFlightDetails" minOccurs="0">
                            <xs:annotation>
